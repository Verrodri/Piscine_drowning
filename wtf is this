#include <stdio.h>

int	ft_iterative_factorial(int nb)
{
	int	i;

	i = 0;
	if (nb == '\0')
	{
		return (1);
	}
	if (nb < 0)
	{
		return (0);
	}
	while (nb > 1)
	{
		nb *= (i - 1);
		i--;
	}
	return (nb);
}

